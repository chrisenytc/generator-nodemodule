{
  "name": "<%= _.slugify(appName) %>",
  "description": "<%= appDescription %>",
  "version": "<%= appVersion %>",
  "homepage": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= _.slugify(appName) %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>/blob/master/LICENSE"
    }
  ],
  "main": "lib/<%= _.slugify(appName) %>",<% if (enableBin) { %>
  "bin": "bin/<%= _.slugify(appName) %>",<% } %>
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "grunt test"
  },
  "dependencies": {
  },
  "devDependencies": {
    "mocha": "~1.10.0",
    "chai": "~1.8.0",
    "supertest": "~0.8.1",
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-contrib-watch": "~0.5.0",
    "grunt-mocha-cli": "~1.3.0",
    "load-grunt-tasks": "~0.2.0",
    "time-grunt": "~0.1.1",
    "jshint-stylish": "~0.1.3",
    "grunt-contrib-watch": "~0.5.0"
  },
  "keywords": []
}
